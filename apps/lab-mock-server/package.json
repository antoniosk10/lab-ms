{
  "name": "lab-mock-server",
  "version": "1.0.0",
  "main": "./src/server.ts",
  "author": "Anvar Abdulsatarov",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "server:dev1": "yarn compile && nodemon ./dist/server.js",
    "server:dev0": "nodemon --exec ts-node --transpile-only ./src/server.ts",
    "server:dev": "cross-env NODE_ENV=development ts-node-dev --experimental-modules --respawn --transpile-only -r tsconfig-paths/register ./src/server.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "type-graphql": "2.0.0-beta.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
