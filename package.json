{
  "name": "lab-next",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "predev": "yarn compile",
    "prestart": "yarn compile",
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "server:dev": "turbo server:dev",
    "full:dev": "concurrently \"yarn server:dev\"  \"yarn dev\" --restart-after 200 --restart-tries 5",
    "lint:all": "turbo lint",
    "compile": "graphql-codegen --config ./config/codegen.ts",
    "watch": "graphql-codegen -w --config ./config/codegen.ts",
    "app:add": "yarn workspace lab-next-app add",
    "app:remove": "yarn workspace lab-next-app remove",
    "server:add": "yarn workspace lab-mock-server add",
    "server:remove": "yarn workspace lab-mock-server remove",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "prepare": "husky install",
    "test": "turbo test -- --silent",
    "test:docker": "turbo test -- --testTimeout=30000",
    "test:snapshot": "turbo test -- --updateSnapshot"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typed-document-node": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^3.0.0",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@next/eslint-plugin-next": "^13.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.0",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.2.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.7",
    "react-test-renderer": "^18.2.0",
    "standard": "^17.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
